    "import subprocess\n",
    "\n",
    "def extract_audio_from_video(video_path,audio_path):\n",
    "    command=[\"ffmpeg\", \"-i\" video_path, \"-vn\" audio_path]\n",
    "    subprocess.run(command,check=True)\n",
    "    return audio_path\n"
   ]
